{
  "name": "alex-fallenstedt-portfolio",
  "version": "1.0.0",
  "description": "Alex Fallenstedt's portfolio website",
  "author": "Alex Fallenstedt <a.fallenstedt@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "npm run build:path && npm run build:posts && nuxt",
    "build:posts": "node ./blogs/index.js",
    "build:path": "node ./path-builder/index.js",
    "build": "npm run build:path &&  npm run build:posts && nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "deploy": "aws s3 sync dist s3://fallenstedt.com",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^17.2.0",
    "imagesloaded": "^4.1.4",
    "node-sass": "^4.9.3",
    "nuxt": "^1.0.0",
    "nuxt-sass-resources-loader": "^2.0.4",
    "sass-loader": "^7.1.0",
    "three": "^0.96.0",
    "vue-my-photos": "^1.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0"
  }
}
